{
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "eject": "expo eject",
        "test": "npx jest --watchAll",
        "postinstall": "yarn prettier && yarn lint",
        "lint": "npx tslint --project tsconfig.json",
        "lint:fix": "npx tslint --project tsconfig.json --fix",
        "prettier": "npx prettier --write 'src/**/*.{tsx,ts,jsx,js,json}'"
    },
    "dependencies": {
        "//": "Not using the original package from archriss because of the onScroll issue when updating the maps, this dev has solved that problem and until the pr gets merged his version with be used. https://github.com/archriss/react-native-snap-carousel/pull/439",
        "@expo/samples": "2.1.1",
        "axios": "^0.18.0",
        "date-fns": "^1.30.1",
        "expo": "^32.0.2-rc.0",
        "lodash": "^4.17.11",
        "react": "16.5.0",
        "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
        "react-native-maps": "^0.23.0",
        "react-native-phone-call": "^1.0.9",
        "react-native-picker-select": "^5.2.3",
        "react-native-render-html": "^3.10.0",
        "react-native-snap-carousel": "Jberivera/react-native-snap-carousel#feature/AllowAnimatedEvent",
        "react-navigation": "^3.1.0",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "@types/date-fns": "^2.6.0",
        "@types/expo": "^31.0.8",
        "@types/jest": "^23.3.13",
        "@types/node": "^10.12.18",
        "@types/react": "^16.7.21",
        "@types/react-native": "^0.57.32",
        "@types/react-native-snap-carousel": "^3.7.1",
        "@types/react-navigation": "^3.0.1",
        "@types/react-test-renderer": "^16.0.3",
        "@types/uuid": "^3.4.4",
        "jest": "^24.0.0",
        "jest-expo": "^32.0.0",
        "prettier": "^1.16.1",
        "react-test-renderer": "^16.7.0",
        "ts-jest": "^23.10.5",
        "tslint": "^5.12.1",
        "tslint-config-airbnb": "^5.11.1",
        "tslint-config-prettier": "^1.17.0",
        "typescript": "^3.2.4"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.(tsx,jsx,ts,js,json)": [
            "yarn prettier",
            "yarn lint:fix",
            "git add"
        ]
    },
    "private": true,
    "jest": {
        "preset": "jest-expo",
        "transform": {
            "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
            "^.+\\.tsx?$": "ts-jest"
        },
        "testMatch": [
            "**/__tests__/**/*.ts?(x)",
            "**/?(*.)+(spec|test).ts?(x)"
        ],
        "moduleFileExtensions": [
            "js",
            "jsx",
            "ts",
            "tsx"
        ],
        "globals": {
            "ts-jest": {
                "tsConfig": {
                    "jsx": "react"
                }
            }
        }
    }
}
